{
	"name": "backend",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "TZ=UTC NODE_ENV=development SWCRC=true nodemon --exec node -r @swc-node/register src/index.ts",
		"test": "TZ=UTC NODE_ENV=test LOG_LEVEL=error SWCRC=true jest --runInBand --detectOpenHandles --forceExit --coverage",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"prepare": "husky",
		"openapi": "openapi-typescript data/openapi/v1.yml -o src/api/types/openapi.d.ts --empty-objects-unknown && prettier --write src/api/types/openapi.d.ts"
	},
	"author": "",
	"license": "MIT",
	"description": "",
	"dependencies": {
		"@prisma/client": "5.16.2",
		"compression": "1.7.4",
		"dotenv": "16.4.5",
		"express": "4.19.2",
		"express-oauth2-jwt-bearer": "1.6.0",
		"express-openapi-validator": "5.2.0",
		"helmet": "7.1.0",
		"pino": "9.2.0",
		"pino-http": "10.2.0",
		"pino-pretty": "11.2.1",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"@side/jest-runtime": "1.1.0",
		"@swc-node/register": "1.10.3",
		"@swc/cli": "0.4.0",
		"@swc/core": "1.6.13",
		"@swc/jest": "0.2.36",
		"@types/compression": "1.7.5",
		"@types/eslint": "8.56.10",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/eslint__js": "8.42.3",
		"@types/express": "4.17.21",
		"@types/express-serve-static-core": "4.19.5",
		"@types/jest": "29.5.12",
		"@types/node": "20.14.10",
		"@types/supertest": "6.0.2",
		"eslint": "9.6.0",
		"eslint-config-prettier": "9.1.0",
		"globals": "15.8.0",
		"husky": "9.0.11",
		"jest": "29.7.0",
		"jest-extended": "4.0.2",
		"lint-staged": "15.2.7",
		"nodemon": "3.1.4",
		"openapi-typescript": "7.0.2",
		"prettier": "3.3.2",
		"prisma": "5.16.2",
		"supertest": "7.0.0",
		"typescript": "5.5.3",
		"typescript-eslint": "7.16.0"
	},
	"prisma": {
		"seed": "node -r @swc-node/register prisma/seed.ts"
	}
}
